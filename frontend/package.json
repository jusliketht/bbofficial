{
  "name": "burnblack-frontend",
  "version": "1.0.0",
  "description": "Burnblack ITR Filing Platform Frontend",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "autoprefixer": "^10.4.14",
    "axios": "^1.5.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.23",
    "lucide-react": "^0.292.0",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.4",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.3.3",
    "xlsx": "^0.18.5",
    "yup": "^1.7.1",
    "zod": "^3.25.76",
    "zustand": "^4.4.1"
  },
  "scripts": {
    "start": "set PORT=3000 && react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format:fix-trailing": "eslint src/ --fix --rule \"no-trailing-spaces: error\"",
    "format:all": "npm run format:fix-trailing && prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "dev": "set PORT=3000 && react-scripts start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.js",
          "**/*.jsx"
        ],
        "rules": {
          "no-unused-vars": "off",
          "import/order": "off",
          "comma-dangle": "off",
          "eol-last": "off",
          "no-trailing-spaces": "off",
          "pascalcase": "off",
          "let-const": "off",
          "prettier/prettier": "off",
          "react-hooks/rules-of-hooks": "off",
          "react-hooks/exhaustive-deps": "off"
        }
      }
    ],
    "rules": {
      "no-unused-vars": "off",
      "import/order": "off",
      "comma-dangle": "off",
      "eol-last": "off",
      "no-trailing-spaces": "off",
      "pascalcase": "off",
      "let-const": "off",
      "prettier/prettier": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "web-vitals": "^2.1.4"
  }
}
